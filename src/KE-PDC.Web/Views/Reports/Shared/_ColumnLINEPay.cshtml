<th data-column-id="no" data-identifier="true" data-css-class="bg-m-blue bg-m-lighten-5 text-center" data-header-css-class="bg-m-blue bg-m-lighten-4" data-header-align="center">@Localizer["No"]</th>
<th data-column-id="transaction_id">@Localizer["Transition Number"]</th>
<th data-column-id="transaction_date" data-type="datetime">@Localizer["Payment Type"]</th>
<th data-column-id="amount" data-type="decimal" data-header-css-class="text-right" data-css-class="text-right">@SharedLocalizer["Amount"]</th>
<th data-column-id="status">@Localizer["Status"]</th>
<th data-column-id="order_id">@Localizer["Order No"]</th>
<th data-column-id="refNo">@Localizer["Ref No"]</th>
<th data-column-id="location_id">@SharedLocalizer["Branch"]</th>
<th data-column-id="return_code">@Localizer["Return Code"]</th>